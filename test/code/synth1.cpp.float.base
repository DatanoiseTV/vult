_ctx_type_0 _Synth1__ctx_type_0_init(){
   _ctx_type_0 _ctx;
   _ctx.x = 0.f;
   return _ctx;
}

_ctx_type_0 _Synth1_smooth_init(){ return _Synth1__ctx_type_0_init();}

float _Synth1_smooth(_ctx_type_0 &_ctx, float input){
   _ctx.x = (_ctx.x + ((input + (- _ctx.x)) * 0.005f));
   return _ctx.x;
}

_ctx_type_1 _Synth1__ctx_type_1_init(){
