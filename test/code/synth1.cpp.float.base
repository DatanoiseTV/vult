   _ctx.pre_x = 0.f;
   return _ctx;
}

_ctx_type_1 _Synth1_change_init(){ return _Synth1__ctx_type_1_init();}

uint8_t _Synth1_change(_ctx_type_1 &_ctx, float x){
   uint8_t v = (_ctx.pre_x != x);
   _ctx.pre_x = x;
   return v;
}

_ctx_type_2 _Synth1__ctx_type_2_init(){
   _ctx_type_2 _ctx;
   _ctx.pre_x = 0;
   return _ctx;
}

_ctx_type_2 _Synth1_edge_init(){ return _Synth1__ctx_type_2_init();}

uint8_t _Synth1_edge(_ctx_type_2 &_ctx, int x){
   uint8_t v = ((_ctx.pre_x != x) && (_ctx.pre_x == 0));
   _ctx.pre_x = x;
   return v;
}

_ctx_type_3 _Synth1__ctx_type_3_init(){
   _ctx_type_3 _ctx;
   _ctx.count = 0;
   return _ctx;
}

_ctx_type_3 _Synth1_each_init(){ return _Synth1__ctx_type_3_init();}

uint8_t _Synth1_each(_ctx_type_3 &_ctx, int n){
   uint8_t ret = (_ctx.count == 0);
   _ctx.count = ((_ctx.count + 1) % n);
   return ret;
}

float _Synth1_pitchToRate(float d){
   return (0.000185392290249f * expf((0.0577623f * d)));
}

_ctx_type_5 _Synth1__ctx_type_5_init(){
   _ctx_type_5 _ctx;
   _ctx.rate = 0.f;
   _ctx.phase = 0.f;
   _ctx._inst0 = _Synth1__ctx_type_1_init();
   return _ctx;
}

_ctx_type_5 _Synth1_phasor_init(){ return _Synth1__ctx_type_5_init();}

float _Synth1_phasor(_ctx_type_5 &_ctx, float pitch, uint8_t reset){
   if(_Synth1_change(_ctx._inst0,pitch)){
      _ctx.rate = _Synth1_pitchToRate(pitch);
   }
   _ctx.phase = (reset?0.f:fmodf((_ctx.phase + _ctx.rate),1.f));
   return _ctx.phase;
}

_ctx_type_6 _Synth1__ctx_type_6_init(){
   _ctx_type_6 _ctx;
   _ctx.phase = 0.f;
   _ctx._inst0 = _Synth1__ctx_type_2_init();
   return _ctx;
}

_ctx_type_6 _Synth1_lfo_init(){ return _Synth1__ctx_type_6_init();}

float _Synth1_lfo(_ctx_type_6 &_ctx, float f, int gate){
   float rate = (0.000226757369615f * f);
   if(_Synth1_edge(_ctx._inst0,gate)){
      _ctx.phase = 0.f;
   }
   _ctx.phase = (_ctx.phase + rate);
   if(_ctx.phase > 1.f){
      _ctx.phase = (_ctx.phase + (-1.f));
   }
   return (sinf((6.28318530718f * _ctx.phase)) + (-0.5f));
}

_ctx_type_7 _Synth1__ctx_type_7_init(){
   _ctx_type_7 _ctx;
   _ctx.volume = 0.f;
   _ctx.pre_phase1 = 0.f;
   _ctx.pitch = 0.f;
   _ctx.n4 = 0.f;
   _ctx.n3 = 0.f;
   _ctx.n2 = 0.f;
   _ctx.n1 = 0.f;
   _ctx.lfo_rate = 0.f;
   _ctx.lfo_amt = 0.f;
   _ctx.gate = 0;
   _ctx.detune = 0.f;
   _ctx.count = 0;
   _ctx._inst5 = _Synth1__ctx_type_0_init();
   _ctx._inst4 = _Synth1__ctx_type_0_init();
   _ctx._inst3 = _Synth1__ctx_type_5_init();
   _ctx._inst2 = _Synth1__ctx_type_0_init();
   _ctx._inst1 = _Synth1__ctx_type_5_init();
   _ctx._inst0 = _Synth1__ctx_type_6_init();
   return _ctx;
}

_ctx_type_7 _Synth1_process_init(){ return _Synth1__ctx_type_7_init();}

float _Synth1_process(_ctx_type_7 &_ctx, float input){
   float lfo_val = (_Synth1_lfo(_ctx._inst0,_ctx.lfo_rate,_ctx.gate) * _ctx.lfo_amt);
   float phase1 = _Synth1_phasor(_ctx._inst1,_ctx.pitch,0);
   float comp = (1.f + (- phase1));
   uint8_t reset = ((_ctx.pre_phase1 + (- phase1)) > 0.5f);
   _ctx.pre_phase1 = phase1;
   float phase2 = _Synth1_phasor(_ctx._inst3,(_ctx.pitch + (_Synth1_smooth(_ctx._inst2,(_ctx.detune + lfo_val)) * 32.f)),reset);
   float sine = sinf((6.28318530718f * phase2));
   float gate_value = ((_ctx.gate > 0)?1.f:0.f);
   return (_Synth1_smooth(_ctx._inst4,_ctx.volume) * sine * comp * _Synth1_smooth(_ctx._inst5,gate_value));
}

_ctx_type_7 _Synth1_noteOn_init(){ return _Synth1__ctx_type_7_init();}

void _Synth1_noteOn(_ctx_type_7 &_ctx, float note, int velocity){
   if(_ctx.count == 0){
      _ctx.n1 = note;
      _ctx.pitch = note;
   }
   else
   {
      if(_ctx.count == 1){
         _ctx.n2 = note;
         _ctx.pitch = note;
      }
      else
      {
         if(_ctx.count == 2){
            _ctx.n3 = note;
            _ctx.pitch = note;
         }
         else
         {
            if(_ctx.count == 3){
               _ctx.n4 = note;
               _ctx.pitch = note;
            }
         }
      }
   }
   if(_ctx.count <= 4){
      _ctx.count = (_ctx.count + 1);
   }
   _ctx.gate = ((_ctx.count > 0)?1:0);
}

_ctx_type_7 _Synth1_noteOff_init(){ return _Synth1__ctx_type_7_init();}

void _Synth1_noteOff(_ctx_type_7 &_ctx, float note){
   uint8_t found = 0;
   if(note == _ctx.n1){
      float _tmp_0 = _ctx.n2;
      float _tmp_1 = _ctx.n3;
      float _tmp_2 = _ctx.n4;
      _ctx.n1 = _tmp_0;
      _ctx.n2 = _tmp_1;
      _ctx.n3 = _tmp_2;
      found = 1;
   }
   else
   {
      if(note == _ctx.n2){
         float _tmp_0 = _ctx.n3;
         float _tmp_1 = _ctx.n4;
         _ctx.n2 = _tmp_0;
         _ctx.n3 = _tmp_1;
         found = 1;
      }
      else
      {
         if(note == _ctx.n3){
            _ctx.n3 = _ctx.n4;
            found = 1;
         }
         else
         {
            if(note == _ctx.n4){
               found = 1;
            }
         }
      }
   }
   if(found && (_ctx.count > 0)){
      _ctx.count = (_ctx.count + (-1));
   }
   _ctx.gate = ((_ctx.count > 0)?1:0);
   if(_ctx.count == 1){
      _ctx.pitch = _ctx.n1;
   }
   if(_ctx.count == 2){
      _ctx.pitch = _ctx.n2;
   }
   if(_ctx.count == 3){
      _ctx.pitch = _ctx.n3;
   }
   if(_ctx.count == 4){
      _ctx.pitch = _ctx.n4;
   }
}

_ctx_type_7 _Synth1_controlChange_init(){ return _Synth1__ctx_type_7_init();}

void _Synth1_controlChange(_ctx_type_7 &_ctx, int control, float value){
   if(control == 30){
      _ctx.volume = (value * 0.00787401574803f);
   }
   if(control == 31){
      _ctx.detune = (value * 0.00787401574803f);
   }
   if(control == 32){
      _ctx.lfo_rate = (value * 0.00787401574803f);
   }
   if(control == 33){
      _ctx.lfo_amt = (2.f * ((value * 0.00787401574803f) + (-0.5f)));
   }
}

_ctx_type_7 _Synth1_default_init(){ return _Synth1__ctx_type_7_init();}

void _Synth1_default_(_ctx_type_7 &_ctx){
