float Filters_lp12(Filters__ctx_type_7 &_ctx, float x, float fc, float q){
   if(Filters_change(_ctx._inst0,fc) || Filters_change(_ctx._inst1,q)){
      float qc = Filters_max(sqrtf(2.f),(q + sqrtf(2.f)));
      fc = Filters_clip(fc,0.f,(22050.f));
      float k = tanf((Filters_pi() * fc * 2.26757369615e-05f));
      float den = ((k * k * qc) + k + qc);
      _ctx.b0 = ((k * k * qc) / den);
      _ctx.b1 = (2.f * _ctx.b0);
      _ctx.b2 = _ctx.b0;
      _ctx.a1 = ((2.f * qc * ((k * k) + (-1.f))) / den);
      _ctx.a2 = (((k * k * qc) + (- k) + qc) / den);
   }
   return Filters_biquad(_ctx._inst2,x,_ctx.b0,_ctx.b1,_ctx.b2,_ctx.a1,_ctx.a2);
}

