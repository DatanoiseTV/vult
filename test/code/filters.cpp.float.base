
#include "filters.h"

float Filters_pi(){
   return 3.1416f;
}

float Filters_min(float a, float b){
   return ((a < b)?a:b);
}

float Filters_max(float a, float b){
   return ((a > b)?a:b);
}

float Filters_clip(float low, float high, float value){
   return Filters_min(Filters_max(low,value),high);
}

