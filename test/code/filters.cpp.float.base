Filters__ctx_type_6 Filters_lp6_init(){ return Filters__ctx_type_6_init();}

float Filters_lp6(Filters__ctx_type_6 &_ctx, float x, float fc){
   if(Filters_change(_ctx._inst0,_ctx.fc)){
      _ctx.fc = Filters_clip(_ctx.fc,0.f,(22050.f));
      _ctx.k = tanf((Filters_pi() * _ctx.fc * 2.26757369615e-05f));
   }
   float b0 = (_ctx.k / (_ctx.k + 1.f));
   float b1 = (_ctx.k / (_ctx.k + 1.f));
   float a1 = ((_ctx.k + (-1.f)) / (_ctx.k + 1.f));
   return Filters_biquad(_ctx._inst1,x,b0,b1,0.f,a1,0.f);
}

