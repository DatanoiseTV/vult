float Filters_biquad(Filters__ctx_type_5 &_ctx, float x, float b0, float b1, float b2, float a1, float a2){
   float w0 = (x + (- (a1 * _ctx.w1)) + (- (a2 * _ctx.w2)));
   float y0 = ((b0 * w0) + (b1 * _ctx.w1) + (b2 * _ctx.w2));
   float _tmp_0 = _ctx.w1;
   float _tmp_1 = w0;
   _ctx.w2 = _tmp_0;
   _ctx.w1 = _tmp_1;
   return y0;
}

