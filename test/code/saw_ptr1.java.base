private float Saw_ptr1_process(Saw_ptr1__ctx_type_0 _ctx, float cv){
   if(Util_change(_ctx._inst13b,cv)){ _ctx.rate = Util_cvToRate(cv); }
   float out = 0.0f; 
   float s1 = _ctx.rate; 
   float dc = -1.f; 
   _ctx.phase = (_ctx.phase + _ctx.rate);
   if(_ctx.phase > 1.f){ _ctx.phase = (-1.f + _ctx.phase); }
   if(_ctx.phase < s1){
      float d = (_ctx.phase / _ctx.rate); 
      out = (2.f + dc + (-2.f * d));
   }
   else
   { out = ((- dc) + (2.f * _ctx.phase)); }
   return out;
}

