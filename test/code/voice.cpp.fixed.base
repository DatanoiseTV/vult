_ctx_type_16 process_init(){ return _ctx_type_16_init();}

int32_t process(_ctx_type_16 &_ctx, int32_t i){
   uint8_t gate = mono_isGateOn(_ctx.monoin);
   int32_t lfo1 = fix_mul(lfo(_ctx._inst0,_ctx.lfo_rate,gate),_ctx.lfo_amt);
   int32_t x = osc(_ctx._inst1,_ctx.pitch,fix_add(_ctx.pw,lfo1),_ctx.wave);
   int32_t amp_env = adsr(_ctx._inst2,gate,_ctx.amp_a,_ctx.amp_d,_ctx.amp_s,_ctx.amp_r);
   int32_t flt_env = adsr(_ctx._inst3,gate,_ctx.env_a,_ctx.env_d,_ctx.env_s,_ctx.end_r);
   int32_t cut_mod = fix_add(_ctx.cut,fix_mul(_ctx.flt_atm,flt_env));
   int32_t output = svf(_ctx._inst4,x,cut_mod,_ctx.res,0);
   return fix_mul(fix_mul(amp_env,output),0x8000 /* 0.500000 */);
}

