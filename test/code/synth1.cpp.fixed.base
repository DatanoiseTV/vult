}

_ctx_type_5 _Synth1_phasor_init(){ return _Synth1__ctx_type_5_init();}

int32_t _Synth1_phasor(_ctx_type_5 &_ctx, int32_t pitch, uint8_t reset){
   if(_Synth1_change(_ctx._inst0,pitch)){
      _ctx.rate = _Synth1_pitchToRate(pitch);
   }
   _ctx.phase = (reset?0x0 /* 0.000000 */:(fix_add(_ctx.phase,_ctx.rate) % 0x10000 /* 1.000000 */));
   return _ctx.phase;
}

_ctx_type_6 _Synth1__ctx_type_6_init(){
   _ctx_type_6 _ctx;
