_ctx_type_16 process_init(){ return _ctx_type_16_init();}

float process(_ctx_type_16 &_ctx, float i){
   uint8_t gate = mono_isGateOn(_ctx.monoin);
   float lfo1 = (lfo(_ctx._inst0,_ctx.lfo_rate,gate) * _ctx.lfo_amt);
   float x = osc(_ctx._inst1,_ctx.pitch,(_ctx.pw + lfo1),_ctx.wave);
   float amp_env = adsr(_ctx._inst2,gate,_ctx.amp_a,_ctx.amp_d,_ctx.amp_s,_ctx.amp_r);
   float flt_env = adsr(_ctx._inst3,gate,_ctx.env_a,_ctx.env_d,_ctx.env_s,_ctx.end_r);
   float cut_mod = (_ctx.cut + (_ctx.flt_atm * flt_env));
   float output = (amp_env * svf(_ctx._inst4,x,cut_mod,_ctx.res,0));
   return output;
}

