function Volume_noteOff(_ctx,note,channel)

end

function Volume_controlChange_init()
   return Volume__ctx_type_1_init();
end

function Volume_controlChange(_ctx,control,value,channel)
   if (control == 30) then
      _ctx.volume = (0.0078741 * real(value));
   end
end

function Volume_default_init()
   return Volume__ctx_type_1_init();
end

function Volume_default(_ctx)
   _ctx.volume = 0.;
end


function process(ctx,input) return Volume_process(ctx,input) end
function noteOn(ctx,note,velocity,channel) return Volume_noteOn(ctx,note,velocity,channel) end
function noteOff(ctx,note,channel) return Volume_noteOff(ctx,note,channel) end
function controlChange(ctx,control,value,channel) return Volume_controlChange(ctx,control,value,channel) end
function init() return Volume_process_init() end
function default(ctx) return Volume_default(ctx) end
