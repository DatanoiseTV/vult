function Noise_pinker(_ctx,x)
   local b0 = 0.027713;
   local b1 = 0.027713;
   local a1 = (-0.944574);
   local w0 = (x + (- (_ctx.w1 * a1)));
   local y0 = ((_ctx.w1 * b1) + (b0 * w0));
   _ctx.w1 = w0;
   return y0;

end

function Noise__ctx_type_1_init()
