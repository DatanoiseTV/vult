function Noise_process(_ctx,color)
   _ctx.x1 = ((7919 + (17389 * _ctx.x1)) % 32768);
   local y1 = (3.06e-05 * real(_ctx.x1));
   _ctx.x2 = ((12553 + (27449 * _ctx.x2)) % 32768);
   local y2 = (3.06e-05 * real(_ctx.x2));
   local out = 0.0;
   if (color > 0.5) then
      out = Noise_pinker(_ctx._inst190,(y1 + (- y2)));
   else

      out = (y1 + (- y2));
   end
   return (2. * out);

end

