void sff_ff::update(void)
{


   audio_block_t * block0 = receiveWritable(0); if(!block0) return;
   audio_block_t * block1 = receiveWritable(1); if(!block1) return;

   for(int i = 0; i < AUDIO_BLOCK_SAMPLES; i++) {
      fix16_t v = Sff_ff_process(data, 0);
      *bp++ = (int16_t)(v / 2);
   }


   transmit(block0, 0);
   transmit(block1, 1);

   release(block0);
   release(block1);
}

