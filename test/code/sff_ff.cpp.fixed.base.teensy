void Sff_ff_process(fix16_t a, fix16_t b, _tuple___real_real__ &_output_){
   _tuple___real_real__ _tuple_5;
   {
      _tuple_5.field_0 = (a + b);
      _tuple_5.field_1 = (a + (- b));
   }
   _output_ = _tuple_5;
   return ;
}

