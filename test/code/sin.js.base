this.Sin_change_init = function() { return this.Sin__ctx_type_0_init();}
this.Sin_change = function(_ctx,x){
   var v = (_ctx.pre_x != x);
   _ctx.pre_x = x;
   return v;
}
this.Sin__ctx_type_1_init = function(){
   var _ctx = {};
   _ctx.y2 = 0.000000;
   _ctx.y1 = 0.000000;
   _ctx.w = 0.000000;
   _ctx.cos_w = 0.000000;
   _ctx._inst2 = this.Sin__ctx_type_0_init();
   this.Sin_sc_osc_start(_ctx);
