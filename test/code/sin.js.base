this.Sin_sc_osc_init = function() { return this.Sin__ctx_type_1_init();}
this.Sin_sc_osc = function(_ctx,pitch){
   var g = 1.000000;
   if(this.Sin_change(_ctx._inst2,pitch)){
      var pre_w = 0.0;
      pre_w = _ctx.w;
      _ctx.w = (0.001165 * this.exp((0.057762 * pitch)));
      g = (this.tan((_ctx.w * 0.500000)) / this.tan((pre_w * 0.500000)));
      _ctx.cos_w = this.cos(_ctx.w);
   }
   var y1_next = ((_ctx.cos_w * _ctx.y1) + ((_ctx.cos_w + 1.000000) * g * _ctx.y2));
   var y2_next = (((_ctx.cos_w + (-1.000000)) * _ctx.y1) + (_ctx.cos_w * g * _ctx.y2));
   _ctx.y1 = this.clip(y1_next,(-1.000000),1.000000);
   _ctx.y2 = this.clip(y2_next,(-1.000000),1.000000);
   return _ctx.y1;
}
this.Sin_sc_osc_start_init = function() { return this.Sin__ctx_type_1_init();}
this.Sin_sc_osc_start = function(_ctx){
   _ctx.y1 = 1.000000;
   _ctx.y2 = ((- _ctx.y1) * 0.500000);
}
this.Sin__ctx_type_2_init = function(){
